import java.util.*;
public class MAIN {

	public static void main(String[] args) {
		int number, digit, Sum = 0;  
		Scanner sc = new Scanner(System.in);  
		System.out.print("Enter the number: ");  
		number = sc.nextInt();  
		while(number > 0)  
		{  
		    
		digit = number % 10;  
		  
		Sum = Sum + digit;  
		 
		number = number / 10;  
		}  
		  
		System.out.println("Sum of Digits: "+Sum); 

	}

}
 
